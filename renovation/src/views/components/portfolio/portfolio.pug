section.portfolio_area
  .container
    .portfolio_filter
      ul.list_style
        li.active(data-filter='*')
          a(href='#') All
        li(data-filter='.commercial')
          a(href='#') Commercial
        li(data-filter='.landscaping')
          a(href='#') Landscaping
        li(data-filter='.newconstruction')
          a(href='#') New Construction
        li(data-filter='.residential')
          a(href='#') Residential
    .row.grid_portfolio_area
      .col-lg-4.col-sm-6.col-12.commercial.landscaping
        .portfolio_item
          a.lightbox(href='../../../assets/images/Smith-residence.jpg')(data-lightbox="example-set")
          .portfolio_img
            a.light
              img.img-fluid(src='../../../assets/images/Smith-residence.jpg' alt='')
          .portfolio_text
            a(href='#')
              h4 Smith Residence
              .view View Project
